{{#notEmpty}}
<div class="cart-header">
    <table class="cart-table">
        <tr>
            <th class="cart-cell cell-check">
                <label class="cart-label">
                    {{#allChecked}}
                    <input type="checkbox" class="cart-select-all" checked/>
                    {{/allChecked}}
                    {{^allChecked}}
                    <input type="checkbox" class="cart-select-all" />
                    {{/allChecked}}
                    <span>全选</span>
                </label>
            </th>
            <th class="cart-cell cell-info">比赛信息</th>
            <th class="cart-cell cell-sponsor">主办方</th>
            <th class="cart-cell cell-price">报名费</th>
            <th class="cart-cell cell-total">合计</th>
            <th class="cart-cell cell-opera">操作</th>
        </tr>
    </table>
</div>
<div class="cart-list">
    {{#cartCompetitionVoList}}
    <table class="cart-table" data-competition-id="{{competitionId}}">
        <tr>
            <td class="cart-cell cell-check">
                <label class="cart-label">
                    {{#competitionChecked}}
                    <input type="checkbox" class="cart-select" checked/>
                    {{/competitionChecked}}
                    {{^competitionChecked}}
                    <input type="checkbox" class="cart-select" />
                    {{/competitionChecked}}
                </label>
            </td>
            <td class="cart-cell cell-img">
                <a class="link" href="./detail.html?competitionId={{competitionId}}">
                    <img class="p-img" src="{{imageHost}}{{competitionMainImage}}" alt="{{competitionName}}" />
                </a>
            </td>
            <td class="cart-cell cell-info">
                <a class="link" href="./detail.html?competitionId={{competitionId}}">{{competitionName}}</a>
            </td>
            <td class="cart-cell cell-sponsor">{{sponsorName}}</td>
            <td class="cart-cell cell-price">￥{{competitionPrice}}</td>
            <td class="cart-cell cell-total">￥{{competitionTotalPrice}}</td>
            <td class="cart-cell cell-opera">
                <span class="link cart-delete">删除</span>
            </td>
        </tr>
    </table>
    {{/cartCompetitionVoList}}
</div>
<div class="cart-footer">
    <div class="select-con">
        <label>
            {{#allChecked}}
            <input type="checkbox" class="cart-select-all" checked/>
            {{/allChecked}}
            {{^allChecked}}
            <input type="checkbox" class="cart-select-all" />
            {{/allChecked}}
            <span>全选</span>
        </label>
    </div>
    <div class="delete-con">
        <span class="link delete-selected">
            <i class="fa fa-trash-o"></i>
            <span>删除选中</span>
        </span>
    </div>
    <div class="submit-con">
        <span>总价：</span>
        <span class="submit-total">￥{{cartTotalPrice}}</span>
        <span class="btn btn-submit">去报名</span>
    </div>
</div>
{{/notEmpty}}
{{^notEmpty}}
<p class="err-tip">
    <span>您的收藏夹空空如也，</span>
    <a href="./index.html">立即去首页</a>
</p>
{{/notEmpty}}